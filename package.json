{
  "name": "seven-bible",
  "version": "0.0.1",
  "description": "Best desktop app to study Bible",
  "productName": "Seven Bible",
  "author": "pvmnsd <thefinalwarningg@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "quasar dev -m electron",
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "build-linux": "quasar build -m electron -T linux",
    "build-windows": "quasar build -m electron -T win",
    "build-mac": "quasar build -m electron -T mac",
    "postinstall": "electron-rebuild -f -w better-sqlite3"
  },
  "dependencies": {
    "@quasar/extras": "^1.0.0",
    "abort-controller": "^3.0.0",
    "adm-zip": "^0.5.9",
    "better-sqlite3": "^8.0.1",
    "electron-fetch": "^1.7.4",
    "font-list": "^1.4.5",
    "fs-extra": "^10.0.1",
    "glob": "^8.0.3",
    "quasar": "^2.6.0",
    "splitpanes": "^3.1.1",
    "vue": "^3.0.0",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.0.0",
    "vue-virtual-scroller": "^2.0.0-alpha.1",
    "vuex": "^4.0.1"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^3.3.1",
    "@quasar/app-vite": "^1.0.0",
    "@types/adm-zip": "^0.5.0",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^12.20.21",
    "@types/splitpanes": "^2.2.1",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "autoprefixer": "^10.4.2",
    "electron": "^22.0.0",
    "electron-builder": "^23.6.0",
    "electron-rebuild": "^3.2.9",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^9.0.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
